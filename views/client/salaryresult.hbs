<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>salaryresult page</title>
    <link rel="stylesheet" href="/salaryresult.css">
  </head>
  <body>
    {{#each user}}
    <div class="container">
        <h1 id="h1tag">salary allocated at <i class="month">{{month}} </i></h1>
        <h2 class="dynamicobj">name: <i>{{name}}</i></h2>
        <h2 class="dynamicobj">salary: <i> {{salary}}</i></h2>
        <h2 class="dynamicobj">month: <i class="month">{{month}}</i></h2>
    </div>
    {{/each}}
  </body>
</html>